	<div class="wrapper">
		<div class="sales_lo">
			<div class="wo_show_wrap">
				<div class="bo_create_cont">
					<div class="bo_create_head"></div>
						<div class="boat_create_p">
							<%= form_for @boat, :html => { :multipart => true } do |b| %>
								<%= b.label :Name %>
								<%= b.text_field :name %>
								<br>
								<br>
								<%= b.label :Container_Limit %>
								<%= b.number_field :container_limit %>
								<br><br>
								<%= b.file_field :avatar %>
						</div>
						<div class="bo_create">
							<%= b.submit :Create, class: 'bo_create_butt' %>
							<% end %>
						</div>
				</div>
				<div class="bo_create_car">
					<div class="bo_slide">
	    				<img src="/assets/boat_s/boat_s_1.png" style="position:absolute;" class="bo_slide_active" />
	    				<img src="/assets/boat_s/boat_s_2.png" style="position:absolute;" />
	    				<img src="/assets/boat_s/boat_s_3.png" style="position:absolute;" />
	    				<img src="/assets/boat_s/boat_s_4.png" style="position:absolute;" />
	    				<img src="/assets/boat_s/boat_s_5.png" style="position:absolute;" />
					</div>
			</div>
			</div>
        			<video autoplay loop id="sales_videobkg" muted poster="">
            		<source src="/assets/vids_home/boats_home2.mp4">
        			</video>
	</div>
</div>
